    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid-layout": "^1.2.0",
    "react-with-styles": "^4.1.0",
    "review-hub": "0.0.19",
    "stream": "0.0.2",
    "zip-js": "0.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",

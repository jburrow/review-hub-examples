    "doc": "typedoc --out ./docs ./src/main.tsx",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "repository": {
    "type": "git",

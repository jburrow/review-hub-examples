    "jest": "^26.6.3",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "style-loader": "^2.0.0",
    "timestamp-webpack-plugin": "^0.2.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.13",
    "typedoc": "^0.20.9",

{
  "name": "review-hub-examples",
  "version": "0.0.1",
  "description": "ReactJs application, components and reducers for a web-based code review tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production && typedoc --out ./docs ./src/main.tsx",
